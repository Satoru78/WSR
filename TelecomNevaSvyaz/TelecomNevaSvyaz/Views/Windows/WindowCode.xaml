<Window x:Class="TelecomNevaSvyaz.Views.Windows.WindowCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TelecomNevaSvyaz.Views.Windows"
        mc:Ignorable="d"
        Title="WindowCode" Height="250" Width="300" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <TextBox Width="200" Name="txbWindowCode" FontSize="20" FontWeight="Bold" IsReadOnly="True"/>
        </StackPanel>
    </Grid>
</Window>
